body { margin: 0; padding: 0; background-image: url(../newimages/menubg.html) repeat-x !important; font-family: "微软雅黑"; color: #666; font-size: 12px; }
input, img, ul, li, form, textarea { margin: 0; padding: 0; }
ul, li { list-style-type: none; }
img { vertical-align:top; border:0px; }

a:link { text-decoration:none !important; }
