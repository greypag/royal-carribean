/* 
	LEVEL ONE
*/
ul.dropdown { position: relative; list-style: none; margin: 0; padding: 0; width: 726px; z-index: 999; }
ul.dropdown li { float: left; }
ul.dropdown li a { font-family: "微软雅黑"; line-height:37px; height: 37px; padding-left: 20px; padding-right: 20px; background:url(../newimages/r_08.jpg) no-repeat right; font-size:14px; font-weight:bold; z-index:999; color: #fff; display: block; }
ul.dropdown li.hover, ul.dropdown li:hover { position: relative; }
ul.dropdown li.hover ul a { font-family: "微软雅黑"; color: #333; text-indent: 0; padding: 6px 14px; height: auto; }
ul.dropdown li.hover ul a:hover { font-family: "微软雅黑"; text-decoration: none; }

/* 
	LEVEL TWO
*/
ul.dropdown ul { width: 160px; background:url(../newimages/r_51.jpg) no-repeat; visibility: hidden; position: absolute; top: 100%; left: 0; list-style: none; margin: 0; padding: 0; z-index: 999; }
ul.dropdown ul li { background:url(../newimages/r_52.jpg) bottom no-repeat; color: #333; float: none; font-size: 12px; width: auto; height: 32px; line-height: 32px; }
ul.dropdown ul li a { font-family: "微软雅黑"; font-size: 12px; height: 32px; }
ul.dropdown ul li a:hover { color:#fbbd00; }
/*
	LEVEL THREE
*/

ul.dropdown ul ul {  background:url(../newimages/r_51.jpg) no-repeat; visibility: hidden; position: absolute; top: -3px; left: 160px; list-style: none; margin: 0; padding: 0; padding-left: 0px; z-index: 1000; float: left; }

/*End Main Menu*/


